<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".test.networktest.TestOkHttpActivity">

    <Button
    android:id="@+id/btn_okhhtp_execute"
    android:text="execute"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_okhhtp_enqueue"
        android:text="enqueue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/btn_okhhtp_login"
        android:text="登录"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/btn_okhhtp_uerinfo"
        android:text="用户信息"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/btn_okhhtp_update_email"
        android:text="修改邮箱"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>